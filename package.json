{
  "name": "install-juliaup",
  "description": "GitHub Action to install juliaup",
  "main": "lib/entrypoint.js",
  "version": "1.0.0-DEV",
  "author": "julia-actions organization, contributors",
  "private": true,
  "homepage": "https://github.com/julia-actions/install-juliaup",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/julia-actions/install-juliaup.git"
  },
  "bugs": {
    "url": "https://github.com/julia-actions/install-juliaup/issues"
  },
  "keywords": [
    "actions",
    "node",
    "setup"
  ],
  "exports": {
    ".": "./dist/index.js"
  },
  "engines": {
    "node" : ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "build": "tsc",
    "pack": "ncc build"
  },
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "verbose": true,
    "clearMocks": true,
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "testMatch": [
      "**/*.test.ts"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ],
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "coverageReporters": [
      "json-summary",
      "text",
      "lcov"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/**"
    ]
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/exec": "^1.1.1",
    "@actions/github": "^6.0.0",
    "@actions/tool-cache": "^2.0.1",
    "async-retry": "^1.3.3",
    "which": "^4.0.0"
  },
  "devDependencies": {
    "@types/async-retry": "^1.4.8",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.2",
    "@types/which": "^3.0.3",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.57.0",
    "eslint-plugin-github": "^4.10.2",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-jsonc": "^2.13.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "make-coverage-badge": "^1.2.0",
    "prettier": "^3.2.5",
    "prettier-eslint": "^16.3.0",
    "ts-jest": "^29.1.2",
    "typescript": "^5.4.2"
  }
}
